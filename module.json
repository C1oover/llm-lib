{
    "id": "llm-lib",
    "title": "LLM Lib",
    "description": "Large Language Model Library that takes an API key to hook into your favorite LLM",
    "authors": [
      {
        "name": "Rachel Schutz"
      }
    ],
    "version": "#{VERSION}#",
    "url": "#{URL}#",
    "manifest": "#{MANIFEST}#",
    "download": "#{DOWNLOAD}#",
    "compatibility": {
        "minimum": "10",
        "verified": "12"
    },
    "scripts": [
        "scripts/llm-lib.js"
    ],
    "languages": [
      {
        "lang": "en",
        "name": "English",
        "path": "languages/en.json"
      }
    ],
    "styles": [
      "styles/llm-lib.css"
    ]
  }